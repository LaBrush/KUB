{% extends "::base.html.twig" %}

{% block title %}
	{{ parent() }} - Modifier la fr√©quence {{ frequence }}
{% endblock %}

{% block body %}
	
	<h2 class="section-title">{{ frequence }}</h2>

	<article class="article">

		{{ form_start(form, { 'attr': {'class': 'formulaire-creation'} }) }}

			{% include "KubEDTBundle:Frequence:form.html.twig" %}

			<span class="validate">
				<input class="button-yes" type="submit">
				<a class="button" href="{{ path('frequence_list') }}">Annuler</a>
				<a class="button-no" href="{{ path("frequence_delete", { id: frequence.id }) }}">Supprimer {{ frequence }}</a>
			</span>
		{{ form_end(form) }}		
			
	</article>
{% endblock %}

{% block js %}

	<script type="text/javascript" src="{{ asset('ressources/jsmin/frequences.min.js') }}"></script>

{% endblock %}