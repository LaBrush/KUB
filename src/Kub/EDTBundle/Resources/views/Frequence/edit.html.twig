{% extends "::base.html.twig" %}

{% block body %}
	
	<article class="article">
		<h2 class="section-title">Modifier la fr√©quence {{ frequence }}</h2>
	</article>

	{{ form_start(form, { 'attr': {'class': 'formulaire-creation'} }) }}

			{% include "KubEDTBundle:Frequence:form.html.twig" %}

			<span class="validate">
				<input class="button-yes" type="submit">
